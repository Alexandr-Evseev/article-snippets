{
    "manifest_version": 2,
    "name": "Performance Watcher",
    "version": "0.1.0",
    "background": {},
    "content_scripts": [
        {
            "matches": "<all_urls>",
            "js": "content-script.js"
        }
    ],
    "browser_actions": {
        "default_icon": "",
        "default_popup": "popup.html",
        "default_title": "PerfWatch"
    },
    "permissions": [
        "activeTab"
    ]
}